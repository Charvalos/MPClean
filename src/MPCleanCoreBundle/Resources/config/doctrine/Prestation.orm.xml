<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\Prestation" table="prestation">
    <indexes>
      <index name="marchandise_prestation" columns="fk_marc"/>
      <index name="marchandise_article" columns="fk_art"/>
    </indexes>
    <id name="idPrestation" type="integer" column="id_prestation">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fkArt" type="integer" column="fk_art" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="libelleServ" type="text" column="libelle_serv" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descriptionServ" type="text" column="description_serv" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="fkMarc" target-entity="Marchandises" fetch="LAZY">
      <join-columns>
        <join-column name="fk_marc" referenced-column-name="id_marc"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="fkOpt" target-entity="OptionPrestation" inversed-by="fkMarc" fetch="LAZY">
      <join-table name="contenir">
        <join-columns>
          <join-column name="fk_marc" referenced-column-name="id_prestation"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="fk_opt" referenced-column-name="id_opt"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
