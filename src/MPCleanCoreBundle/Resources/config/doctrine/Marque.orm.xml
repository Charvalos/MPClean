<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\Marque" table="marque">
    <unique-constraints>
      <unique-constraint name="ind_id_marque" columns="id_marq"/>
    </unique-constraints>
    <id name="idMarq" type="integer" column="id_marq">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleMarq" type="text" column="libelle_marq" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
