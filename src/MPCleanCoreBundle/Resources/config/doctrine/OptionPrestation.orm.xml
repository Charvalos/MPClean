<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\OptionPrestation" table="option_prestation">
    <unique-constraints>
      <unique-constraint name="ind_id_option" columns="id_opt"/>
    </unique-constraints>
    <id name="idOpt" type="integer" column="id_opt">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleOpt" type="text" column="libelle_opt" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="descriptionOpt" type="text" column="description_opt" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-many field="fkMarc" target-entity="Prestation" mapped-by="fkOpt" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
