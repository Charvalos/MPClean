<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\Commande" table="commande">
    <indexes>
      <index name="ind_comm_etat" columns="en_cours_comm"/>
      <index name="marchandises_faire_partie_de_commande" columns="fk_marc"/>
      <index name="commande_passe_par_client" columns="fk_cli"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="ind_pk_commande" columns="id_comm"/>
    </unique-constraints>
    <id name="idComm" type="integer" column="id_comm">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateComm" type="date" column="date_comm" nullable="false"/>
    <field name="enCoursComm" type="boolean" column="en_cours_comm" nullable="true"/>
    <field name="qteComm" type="integer" column="qte_comm" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="fkCli" target-entity="Client" fetch="LAZY">
      <join-columns>
        <join-column name="fk_cli" referenced-column-name="id_cli"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fkMarc" target-entity="Marchandises" fetch="LAZY">
      <join-columns>
        <join-column name="fk_marc" referenced-column-name="id_marc"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
