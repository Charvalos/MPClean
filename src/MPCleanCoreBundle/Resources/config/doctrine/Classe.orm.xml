<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\Classe" table="classe">
    <unique-constraints>
      <unique-constraint name="in_id_classe" columns="id_classe"/>
    </unique-constraints>
    <id name="idClasse" type="integer" column="id_classe">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleClasse" type="text" column="libelle_classe" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
