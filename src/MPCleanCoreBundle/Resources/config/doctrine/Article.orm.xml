<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\Article" table="article">
    <unique-constraints>
      <unique-constraint name="ind_id_article" columns="id_art"/>
    </unique-constraints>
    <id name="idArt" type="integer" column="id_art">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="artLibelle" type="text" column="art_libelle" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
