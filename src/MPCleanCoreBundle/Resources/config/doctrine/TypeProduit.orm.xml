<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\TypeProduit" table="type_produit">
    <unique-constraints>
      <unique-constraint name="pk_type" columns="id_type_produit"/>
    </unique-constraints>
    <id name="idTypeProduit" type="integer" column="id_type_produit">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="libelleTyp" type="text" column="libelle_typ" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
