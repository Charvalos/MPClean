<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\Vehicule" table="vehicule">
    <indexes>
      <index name="fk_classe" columns="fk_classe"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="ind_id_vehicule" columns="id_vhc"/>
    </unique-constraints>
    <id name="idVhc" type="integer" column="id_vhc">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="fkClasse" type="integer" column="fk_classe" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="libelleVhc" type="text" column="libelle_vhc" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
