<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MPCleanCoreBundle\Entity\Lier" table="lier">
    <indexes>
      <index name="IDX_B133E8FAA1A6453E" columns="fk_prestation"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="media_prestation" columns="fk_prestation,fk_med"/>
      <unique-constraint name="ind_fk_media_prestation" columns="fk_med"/>
    </unique-constraints>
    <id name="idLier" type="integer" column="id_lier">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="fkMed" target-entity="Media" fetch="LAZY">
      <join-columns>
        <join-column name="fk_med" referenced-column-name="id_med"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="fkPrestation" target-entity="Prestation" fetch="LAZY">
      <join-columns>
        <join-column name="fk_prestation" referenced-column-name="id_prestation"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
